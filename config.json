{
	"description": "netmap-docker-driver - veth network driver for netmap-based networks in Python",
	"documentation": "https://github.com/sergio-gimeenz/netmap-docker-driver",
	"workdir": "/usr/src/app",
	"entrypoint": [
		"./run.py"
	],
	"env": [
		{
			"name": "ENVIRONMENT",
			"value": "production"
		},
		{
			"name": "HOME",
			"value": "/usr/src/app"
		}
	],
	"interface": {
		"types": [
			"docker.networkdriver/1.0"
		],
		"socket": "netmap-docker-driver.sock"
	},
	"linux": {
		"capabilities": [
			"CAP_NET_ADMIN"
		]
	},
	"network": {
		"type": "host"
	}
}